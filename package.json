{
  "name": "juxt",
  "version": "1.0.0",
  "description": "Grocery price comparison app",
  "main": "index.js",
  "scripts": {
    "legacy": "npm run migrate:latest && babel-node ./src/scripts/run.js",
    "migrate:latest": "babel-node ./node_modules/.bin/knex migrate:latest",
    "migrate:rollback": "babel-node ./node_modules/.bin/knex migrate:rollback",
    "migrate:redo": "npm run migrate:rollback && npm run migrate:latest",
    "seed:run": "babel-node ./node_modules/.bin/knex seed:run",
    "src": "babel src --presets es2015-node4,stage-0 --plugins transform-runtime --out-dir lib",
    "pack": "webpack --config=config/webpack.js",
    "build": "npm run src && npm run pack",
    "watch-pack": "webpack-dev-server --inline --hot --config=./config/webpack.js --content-base ./public",
    "watch-src": "babel --presets es2015-node4,stage-0 -w src --out-dir lib",
    "watch-server": "nodemon ./lib/server/index.js --exec babel-node",
    "test": "JUXT_ENV='test' mocha --require ./test/setup ./test",
    "watch-test-server": "mocha -w --require ./test/setup ./test/server",
    "watch-test-ui": "mocha -w --require ./test/setup --require ./test/ui/test_helper ./test/ui",
    "dev": "concurrently 'npm run watch-src' 'npm run watch-pack' 'npm run watch-server'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathanhuang13/juxt.git"
  },
  "author": "Jonathan Huang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonathanhuang13/juxt/issues"
  },
  "homepage": "https://github.com/jonathanhuang13/juxt#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.0",
    "bookshelf": "^0.9.5",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "immutable": "^3.8.1",
